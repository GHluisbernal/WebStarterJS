{
  "name": "web-starter-js",
  "version": "1.0.0",
  "description": "Own workflow for web",
  "main": "appServe.js",
  "author": "Luis",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "lighthouse": "^1.6.0",
    "localtunnel": "^1.8.2",
    "opn": "^4.0.2",
    "compression": "^1.6.2",
    "chalk": "^1.1.3",
    "express": "^4.15.2",
    "whatwg-fetch": "^2.0.3",
    "json-server": "^0.9.6",
    "json-schema-faker": "^0.4.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-preset-latest": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-register": "^6.24.0",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-md5-hash": "^0.0.5",
    "html-webpack-plugin": "^2.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "mocha": "^3.2.0",
    "chai": "^3.5.0",
    "jsdom": "^9.12.0",
    "eslint": "^3.17.1",
    "babel-eslint": "^7.1.1",
    "eslint-watch": "^3.0.1",
    "style-loader": "^0.13.2",
    "css-loader": "^0.27.3"
  },
  "dependencies": {},
  "scripts": {
    "prestart": "babel-node buildScripts/preStart",
    "start": "npm-run-all --parallel security-check serve:app lint:watch test:watch start-mockapi",
    "serve:app": "babel-node buildScripts/appServer.js",
    "lint": "esw app buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "lighthouse": "lighthouse http://localhost:3000",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"app/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node buildScripts/generateMockData.js",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch app/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js"
  }
}
